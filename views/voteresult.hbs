<div class="container-fluid rounded-sm border border-light shadow bg-white">
    <div class="px-5 py-5">
        <div class="container">
            <div class="col justify-content-md-center">
                <div>
                    <a href="/votelist" loading="lazy" type="button" class="btn btn-outline-dark"><svg
                            xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                            class="bi bi-arrow-left-short" viewBox="0 0 16 16">
                            <path fill-rule="evenodd"
                                d="M12 8a.5.5 0 0 1-.5.5H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5a.5.5 0 0 1 .5.5z" />
                        </svg></a>
                </div>
                <div class="card">
                    <div class="card-header">
                        Kết quả bầu cử
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">Các ứng cử viên:</h5>
                        {{#each data}}
                        <p class="card-text">- {{this.name}}: {{this.count}} phiếu bầu</p>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

